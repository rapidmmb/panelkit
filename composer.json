{
    "name": "mmb/panelkit",
    "description": "Panel kit for mmb",
    "type": "library",
    "authors": [
        {
            "name": "Mahdi Saremi",
            "email": "mahdisarmi00@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Rapid\\Mmb\\Providers\\PanelKitServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Rapid\\Mmb\\PanelKit\\": "app/",
            "Rapid\\Mmb\\PanelKit\\Database\\Factories\\": "database/factories/",
            "Rapid\\Mmb\\PanelKit\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rapid\\Mmb\\PanelKit\\Tests\\": "tests/"
        }
    },
    "require": {
        "laravel/framework": "^11.0",
        "mmb/mmb": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "11.0@dev"
    }
}
